        }
      },
      "root": "projects/paulrichez/ng-gallery-inline",
      "sourceRoot": "projects/paulrichez/ng-gallery-inline/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/paulrichez/ng-gallery-inline/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/paulrichez/ng-gallery-inline/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/paulrichez/ng-gallery-inline/tsconfig.lib.json"
            }
